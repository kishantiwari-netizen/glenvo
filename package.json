{
  "name": "glenvo-auth",
  "version": "1.0.0",
  "description": "GlenvoShip Authentication Module",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:simple": "ts-node-dev --respawn --transpile-only src/server-simple.ts",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:reset": "sequelize-cli db:migrate:undo:all && sequelize-cli db:migrate",
    "seed": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo:all",
    "seed:permissions": "sequelize-cli db:seed --seed 20231201000000-demo-permissions.js && sequelize-cli db:seed --seed 20231201000000-demo-roles.js && sequelize-cli db:seed --seed 20231201000002-demo-role-permissions.js && sequelize-cli db:seed --seed 20231201000001-demo-users.js",
    "db:test": "node test-db.js",
    "db:setup": "npm run migrate && npm run seed",
    "db:reset": "npm run migrate:reset && npm run seed",
    "db:setup:permissions": "npm run migrate && npm run seed:permissions"
  },
  "keywords": [
    "authentication",
    "glenvo",
    "shipping"
  ],
  "author": "GlenvoShip Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.6.5",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.35.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "@types/sequelize": "^4.28.19",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "sequelize-cli": "^6.6.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  }
}
